{
    "root": true,
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/stylistic"
    ],
    "env": {
      "es2021": true,
      "node": true
    },
    "rules": {
      "no-console": 1,
      "@typescript-eslint/no-var-requires": 0
    }
  }
  